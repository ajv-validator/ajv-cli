{
  "name": "ajv-cli",
  "version": "2.0.0",
  "description": "A command line interface for epoberezkin/ajv JSON schema validator",
  "scripts": {
    "eslint": "eslint index.js commands/*.js test/*.js test/**/*.js",
    "test-spec": "mocha test/*.spec.js -R spec",
    "test-cov": "nyc npm run test-spec",
    "test": "npm run eslint && npm run test-cov"
  },
  "nyc": {
    "exclude": [
      "test",
      "node_modules"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true
  },
  "bin": {
    "ajv": "index.js"
  },
  "preferGlobal": true,
  "author": "Jesse Collis <jesse@jcmultimedia.com.au>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jessedc/ajv-cli"
  },
  "dependencies": {
    "ajv": "^5.0.0",
    "ajv-pack": "^0.3.0",
    "fast-json-patch": "^0.5.6",
    "glob": "^7.0.3",
    "json-schema-migrate": "^0.2.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "ajv-keywords": "^2.0.0",
    "coveralls": "^2.11.8",
    "eslint": "^2.4.0",
    "mocha": "^2.4.5",
    "nyc": "^8.3.0",
    "pre-commit": "^1.1.2"
  }
}
