{
  "extends": "@ajv-validator/config",
  "include": ["src"],
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["ES2018"],
    "types": ["node"],
    "allowJs": true,
    "target": "ES2018",
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "useUnknownInCatchVariables": false
  }
}
